@page
@model FirstWebExample.Pages.PersonInfoModel
@{
    string name = "Martin";
    int age = 22;
}
<h1>Person Details</h1>
<fieldset>
    <legend>Person's Information</legend>
    <form>
        <table border="1">
            <tr>
                <td>Name:</td>
                <td><input type="text" name="txtname" value="@name" readonly /></td>
            </tr>
            <tr>
                <td>Age:</td>
                <td><input type="number" name="txtage" value="@age" readonly /></td>
            </tr>
            <tr>
                <td><input type="button" value="Submit" /></td>
                <td><input type="reset" value="Reset" /></td>
            </tr>
        </table>
    </form>
</fieldset>

